@startuml

class House {
  - catalog:Catalog
  +getCatalog()
}

class Catalog {
  - sensorList : List<String>
  + addSensorType(...) : boolean
}

class AddSensorTypeController {
    - house : House

    + AddSensorTypeController(...)
    + defineSensorType(...) : boolean
}

House "1" *--"1" Catalog : has
AddSensorTypeController "1" ..> "1" House : uses

@enduml